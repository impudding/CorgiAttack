#include"tower.h"
#include<QPixmap>
#include<QVector>
#include<QPointF>

Tower::Tower(QGraphicsItem *parent)
{
    //set the graphics
    setPixmap(QPixmap(""));    

    QVector<QPointF> points;
    points << QPoint(1,0) << QPoint(2,0) << QPoint(3,1) << QPoint(3,2)  << QPoint(2,3)  << QPoint(1,3) << QPoint(0,2) << QPoint(0,1)
}
